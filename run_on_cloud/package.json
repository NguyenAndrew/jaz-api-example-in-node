{
  "name": "run_on_cloud",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ci:backend": "cd ../back_end && npm ci --production=false",
    "ci:frontend": "cd ../front_end && npm ci --production=false",
    "prune:backend": "cd ../back_end && npm prune --production",
    "prune:frontend": "cd ../front_end && npm prune --production",
    "install:backend": "cd ../back_end && npm install",
    "install:frontend": "cd ../front_end && npm install",
    "test:backend": "cd ../back_end && npm run test",
    "test:frontend": "cd ../front_end && npm run test -- --watchAll=false",
    "build:backend": "cd ../back_end && npm run build",
    "build:frontend": "npm run build:frontend:linux",
    "build:frontend:windows": "cd ../front_end && npm run build:windows",
    "build:frontend:linux": "cd ../front_end && npm run build:linux",
    "build:frontend:mac": "cd ../front_end && npm run build:mac",
    "start": "cd ../back_end && npm run start",
    "start:windows": "cd ../back_end && npm run start:cloud:windows",
    "start:linux": "cd ../back_end && npm run start:cloud:linux",
    "start:mac": "cd ../back_end && npm run start:cloud:mac"
  },
